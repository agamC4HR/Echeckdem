@model List<Echeckdem.CustomFolder.OrganisationsListViewModel>

<h2>Active Organizations</h2>

<table class="table">
    <thead>
        <tr>
            <th>S.No</th>
            <th>Organization Name</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <td>@(i + 1)</td>
                <td>
                    <a asp-action="ViewLocations" asp-route-oid="@Model[i].oid">
                        @Model[i].Oname
                    </a>
                </td>
            </tr>
        }
    </tbody>
</table>